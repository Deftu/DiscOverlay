    "productName": "discoverlay",
    "version": "0.1.0",
    "identifier": "dev.deftu.discoverlay",
    "plugins": {},
    "app": {
        "windows": [
            {
                "minWidth": 1280,
                "minHeight": 720,
                "center": true,
                "fullscreen": false,
                "resizable": false,
                "title": "DiscOverlay",
